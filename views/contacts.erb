<h2>There are <%= @contacts.count %> contacts</h2>

<section id="contact-section" class="contacts">
  <% @contacts.each do |contact| %>
    <ul class="people">
      <li><%= contact.full_name %> (<%= contact.email %>)</li>
      <li class="modify"><a href="#">Edit</a></li>
      <li class="modify">|</li>
      <li class="modify"><a href="#">Delete</a></li>
    </ul>
  <% end %>
</section>
